{
  "format": "custom",
  "id": "blog_post",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "heading1"
        }
      },
      "excerpt": {
        "type": "StructuredText",
        "config": {
          "label": "Excerpt",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph"
        }
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Categoria",
          "placeholder": "",
          "options": [
            "Gestão Escolar",
            "Marketing",
            "Retenção",
            "Tecnologia",
            "Vendas"
          ],
          "default_value": "Gestão Escolar"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "rich_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
